@model IEnumerable<ShisheVere.Models.Orders>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Emri)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Mbiemri)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Adresa)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Shishe)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Sasia)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Emri)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Mbiemri)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Adresa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Shishe)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sasia)
            </td>
            <td>
                @Html.ActionLink("Delete", "DeleteOrder", new { id = item.id })
            </td>
        </tr>
    }

</table>